<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="TCPEndpoint" />
            </netTcpBinding>
            <wsHttpBinding>
                <binding name="HTTPEndpoint" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://192.168.202.212:7777/Calc" binding="wsHttpBinding"
                bindingConfiguration="HTTPEndpoint" contract="CalcBusinessLogicProxy.ICalcBusinessLogic"
                name="HTTPEndpoint">
                <identity>
                    <userPrincipalName value="ORSK\Admin" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://192.168.202.212:7778/Calc" binding="netTcpBinding"
                bindingConfiguration="TCPEndpoint" contract="CalcBusinessLogicProxy.ICalcBusinessLogic"
                name="TCPEndpoint">
                <identity>
                    <userPrincipalName value="ORSK\Admin" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>